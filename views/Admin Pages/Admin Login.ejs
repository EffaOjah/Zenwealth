</head>

<body class="bg-one">

  <div class="login-back-button px-3 py-3" style="cursor: pointer;">
    <i onclick="goBack()" class="fa-solid fa-arrow-left" style="color: #fff;"></i>
  </div>

 
  <!-- Login Wrapper Area -->
  <form action="/admin-login" method="post">
  <div class="login-wrapper d-flex align-items-center justify-content-center">
    <div class="custom-container">
      <div class="text-center px-4">
        <img class="login-intro-img" src="/img/logo (3).png" style="height: 80px;" alt="">
      </div>

      <!-- Register Form -->
      <div class="register-form mt-4">
        <h6 class="mb-3 text-center color-one">Log in as admin...</h6>

        
          <div class="form-group mb-3">
            <input class="form-control py-2" type="text" id="username" name="username" placeholder="Username" required>
          </div>

          <div class="form-group position-relative">
            <input class="form-control py-2" id="psw-input" name="password" type="password" placeholder="Enter Password" required>
            <div class="position-absolute" id="password-visibility">            
            </div>
          </div>
          <!-- <a href="Dashboard.html">
            <button class="btn primary-btn w-100 text-white mt-3 fw-bold">Sign In</button>
          </a> -->
          <button class="btn btn-color w-100 text-white mt-3 fw-bold">Log In</button>
         
        
      </div><br>
    </div>
  </div>
</form>

<span id="serverError" class="d-none"><%= locals.errorMessage %></span>

 <!--build:js-->
 <script src="/bootstrap-5.0.2-dist/js/bootstrap.bundle.min.js"></script>
 <script src="jquery-3.6.0.min.js.download"></script>

 <!-- Toastify JS -->
 <script type="text/javascript" src="/js/toastify.js"></script>

 <script src="app.js.download2"></script>
